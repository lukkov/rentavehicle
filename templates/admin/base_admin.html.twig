<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Dashboard{% endblock %}</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    {% block stylesheets %}
        {{ encore_entry_link_tags('admin') }}
    {% endblock %}
</head>
<body>
    <div class="wrapper ">
        {% include 'admin/_partials/_sidebar.html.twig' %}
        <div class="main-panel">
            {% include 'admin/_partials/_navbar.html.twig' %}
            <div class="content">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-md-12">
                            {% include 'admin/_common/_flash_messages.html.twig' %}

                            {% block body %}

                            {% endblock %}
                        </div>
                        {% include 'admin/_common/_confirm_delete_window.html.twig' %}
                    </div>
                </div>
            </div>
            {% include 'admin/_partials/_footer.html.twig' %}
        </div>
    </div>
    {% block javascripts %}
        {{ encore_entry_script_tags('admin') }}
    {% endblock %}
</body>
</html>
