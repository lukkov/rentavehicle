<!DOCTYPE html>
<html lang="en">
<head>
    <title>{% block title %} Rentavehicle {% endblock %}</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link href="https://fonts.googleapis.com/css?family=DM+Sans:300,400,700&display=swap" rel="stylesheet">

    {% block stylesheets %}
        {{ encore_entry_link_tags('front') }}
    {% endblock %}

</head>

<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">

    <div class="site-wrap" id="home-section">

        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>

        {% include 'front/_partials/_header.html.twig' %}
        {% block body %}

        {% endblock %}

        {% include 'front/_partials/_how_it_works.html.twig' %}

        {% include 'front/_partials/_footer.html.twig' %}

    </div>

    {% block javascripts %}
        {{ encore_entry_script_tags('front') }}
    {% endblock %}

</body>

</html>
